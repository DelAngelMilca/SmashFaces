// document = es un selector
function

$(document).ready(function(){
    $('#imagenPersona').attr('src','fotos/ANA.jpg');


    $('#btnclick').click(function(){
    	//obtener nombre
    	var nombre =$('#inputNombre').val();
    	var cadena = nombre.toUpperCase();
    	alert("Ingresaste"+" "+cadena);

    	
    });
});

